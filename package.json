{
  "name": "@habx/ui-table",
  "version": "4.6.0",
  "description": "React Table components",
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/typings/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
